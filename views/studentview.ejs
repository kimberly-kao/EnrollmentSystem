                                </div>
                                <div class="row attribute" id="semester">
                                    Semester: <%= student[i].Semester %>
                                </div>
                                <div class="row attribute" id="degree">
                                    Degree Type: <%= student[i].DegreeType %>
                                </div>
                                <div class="row attribute" id="test">
                                    Test: <%= student[i].APScores %>
                                </div>
                                <div class="row attribute" id="sat">
                                    SAT: <%= student[i].SAT %>
                                </div>
                                <div class="row attribute" id="submission">
                                    Submission Date: <%= student[i].SubmissionDateTime %>
                                </div>
                                <div class="row attribute" id="military">
                                    Military Status: <%= student[i].MilitaryStatus %>
                                </div>
                                <div class="row attribute" id="piq">
                                    PIQ: <%= student[i].PIQ %>
                                </div>
                                <div class="row attribute" id="status">
                                    Status: <%= student[i].ApplicationStatus %>
                                </div>
                                <div class="row attribute" id="transcripts">
                                    Transcripts: <%= student[i].Transcripts %>
                                </div>
                                <div class="row attribute" id="extracurriculum">
                                    Extracurricular Activity: <%= student[i].Extracurriculum %> 
                                </div>
                                <div class="row attribute" id="lofrec">
                                    Letter of Rec: <%= student[i].LetterOfRec %>
                                </div>
                            </div>
