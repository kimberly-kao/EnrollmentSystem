                                    First Name: <%= student[i].FirstName %>
                                </div>
                                <div class="row attribute" id="mname">
                                    Middle Name: <%= student[i].MiddleName %>
                                </div>
                                <div class="row attribute" id="lname">
                                    Last Name: <%= student[i].LastName %>
                                </div>
                                <div class="row attribute" id="address">
                                    Address: <%= student[i].StudentAddress %>
                                </div>
                                <div class="row attribute" id="bday">
                                    Birthday: <%= student[i].Birthday %>
                                </div>
                                <div class="row attribute" id="email">
                                    Email: <%= student[i].StudentEmail %>
                                </div>
                                <div class="row attribute" id="telephone">
                                    Telephone: <%= student[i].Telephone %>
                                </div>
                                <div class="row attribute" id="gender">
                                    Gender: <%= student[i].Gender %>
                                </div>
                                <div class="row attribute" id="attribute">
                                    School: <%= student[i].PreviousSchool %>
